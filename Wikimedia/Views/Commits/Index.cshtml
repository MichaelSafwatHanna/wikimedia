@model IEnumerable<Wikimedia.Models.Commit>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h5>Commits</h5>
<ul class="list-group">
    @foreach (var commit in Model)
    {
        <li class="list-group-item list-group-item-action">
            <header>
                <a class="text-dark" href="#">
                    @commit.Message
                </a>
            </header>
            <a class="text-dark" href="#">
                <b>@commit.Author.Username</b>
            </a>
            <small> Commited on @commit.Date</small>
        </li>
    }
</ul>